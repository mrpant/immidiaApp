.box-padding {
  padding: 10px;
}

.se-pre-con {
  position: fixed;
  left: 0px;
  top: 0px;
  width: 100%;
  height: 100%;
  z-index: 999999;
  background-color: rgb(80, 107, 134);
  background: #a2acab center no-repeat;
}

.loadiconbg {
  margin-top: 10%;
  text-align: center;
  max-width: 100%;
  height: auto;
}

.loadiconbg img {
  -webkit-animation: rot 4s linear 0s infinite;
  -moz-animation: rot 4s linear 0s infinite;
  -o-animation: rot 4s linear 0s infinite;
  animation: rot 4s linear 0s infinite;
}

.center-block {
  display: block;
  margin-right: auto;
  margin-left: auto;
}

.loading {
  margin-top: 1%;
  text-align: center;
  max-width: 100%;
  height: auto;
  color: #fff;
  font-size: 30px;
}

@-webkit-keyframes rot {
  from {
    transform: rotate(0deg)
  }
  to {
    transform: rotate(360deg)
  }
  @-moz-keyframes rot {
    from {
      transform: rotate(0deg)
    }
    to {
      transform: rotate(360deg)
    }
    @-o-keyframes rot {
      from {
        transform: rotate(0deg)
      }
      to {
        transform: rotate(360deg)
      }
      @keyframes rot {
        from {
          transform: rotate(0deg)
        }
        to {
          transform: rotate(360deg)
        }


