}

.header .toolbar-background-md {
  background: #19253b;
}

.header .logo {
    position: absolute;
    left: 15px;
    top: 4px;
    width: 28px;
    margin: 1em;
}

.header-md .logo {
  top: 0;
  left: 10px;
}
.header-ios .logo {
  top: 15px;
  left: 25px;
}

.header .right-hdr {
  position: absolute;
  right: 20px;
  top: 30px;
